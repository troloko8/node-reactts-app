    "private": true,
    "dependencies": {
        "@tanstack/react-query": "^5.55.4",
        "@tanstack/react-query-devtools": "^5.55.4",
        "@testing-library/jest-dom": "^5.17.0",
        "@testing-library/react": "^13.4.0",
        "@testing-library/user-event": "^13.5.0",
        "@types/jest": "^27.5.2",
        "@types/node": "^16.18.108",
        "@types/react": "^18.3.5",
